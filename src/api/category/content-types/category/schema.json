{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": "Organize your content into categories"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "text"
    },
    "icon": {
      "type": "string",
      "required": true,
      "default": "default"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.service-page",
        "shared.articles"
      ]
    },
    "keywords": {
      "type": "text"
    },
    "breadcrumbs": {
      "type": "dynamiczone",
      "components": [
        "shared.service-page"
      ]
    }
  }
}
